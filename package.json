    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "^18.0.8",
    "@zeit/next-source-maps": "0.0.3",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.2",
    "eslint": "8.26.0",
